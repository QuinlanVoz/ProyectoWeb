<p id="notice"><%= notice %></p>

<%= content_for :head do%>
	<title>Proyectos</title>
<% end %>

<%= content_for :intro do%>
	<h3>Proyectos</h3>
<% end %>

<p>Nuestros Proyectos</p>

<div class="container">
    <div class="row">
    <% @proyectos.each do |proyecto| %>
        <div class="col-3">
        <br>
        <br>
            <div id="carouselExampleControls1" class="carousel slide" data-ride="carousel">
                <div class="carousel-inner">
                    <div class="carousel-item active">
                        <img class="d-block w-100" src="https://img10.naventcdn.com/avisos/11/00/59/11/47/03/360x266/224598396.jpg" alt="First slide">
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100" src="https://img10.naventcdn.com/avisos/11/00/59/11/47/03/360x266/224598398.jpg" alt="Second slide">
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100" src="https://img10.naventcdn.com/avisos/11/00/59/11/47/03/360x266/224598399.jpg" alt="Third slide">
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100" src="https://img10.naventcdn.com/avisos/11/00/59/11/47/03/360x266/224598400.jpg" alt="Third slide">
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100" src="https://img10.naventcdn.com/avisos/11/00/59/11/47/03/360x266/224598397.jpg" alt="Third slide">
                    </div>
                    <a class="carousel-control-prev" href="#carouselExampleControls1" role="button" data-slide="prev">
                        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                        <span class="sr-only">Previous</span>
                    </a>
                    <a class="carousel-control-next" href="#carouselExampleControls1" role="button" data-slide="next">
                        <span class="carousel-control-next-icon" aria-hidden="true"></span>
                        <span class="sr-only">Next</span>
                    </a>
                </div>
            </div>
        </div>
        <div class="col-9">
          <br>      
            <a class="nav-link" href="/static/departamentos">
                <%= proyecto.nombre %>
                <hr>
                <div class="text-justify"><%= proyecto.caracteristicas %></div>
            </a>
         </div>
        <% end %>
    </div>
</div>

