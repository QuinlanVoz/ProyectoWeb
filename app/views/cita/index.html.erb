<p id="notice"><%= notice %></p>

<h1>Cita</h1>

<table>
  <thead>
    <tr>
      <th>Codigo</th>
      <th>Nombre</th>
      <th>Apellido</th>
      <th>Dni</th>
      <th>Telefono</th>
      <th>Fecha</th>
      <th>Hora</th>
      <th>Estado</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cita.each do |citum| %>
      <tr>
        <td><%= citum.codigo %></td>
        <td><%= citum.nombre %></td>
        <td><%= citum.apellido %></td>
        <td><%= citum.dni %></td>
        <td><%= citum.telefono %></td>
        <td><%= citum.fecha %></td>
        <td><%= citum.hora %></td>
        <td><%= citum.estado %></td>
        <td><%= link_to 'Show', citum %></td>
        <td><%= link_to 'Edit', edit_citum_path(citum) %></td>
        <td><%= link_to 'Destroy', citum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Citum', new_citum_path %>
